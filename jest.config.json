{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "roots": ["<rootDir>/__tests__"],
  "testMatch": ["**/__tests__/**/*.test.ts"],
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "modulePathIgnorePatterns": ["<rootDir>/node_modules/"],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": ["text", "lcov", "html"]
}
